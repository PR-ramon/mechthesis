@article{Fahland.2019,
	title = {Flow {Control} for {Turbulent} {Skin}-friction drag reduction on airfoils},
	abstract = {The present thesis is focused on turbulent boundary layer control on airfoils. In particular, uniform blowing and suction is investigated regarding its inﬂuence on aerodynamic properties of airfoils covering a vast parameter space. Variations include: Reynolds number of Re = [105, 107], angle of attack of α = [−3◦, 12◦], airfoil shape, control region and size as well as the control intensity. The goal of these investigations is not to optimize a single conﬁguration but to gain knowledge about beneﬁts and costs regarding the overall airfoil performance depending on the parameters considered. The calculations are done using Reynolds-Averaged Navier-Stokes equations. The OpenFOAM CFD-toolbox is used with the steady state solver simpleFoam and ﬂow is considered incompressible. Turbulence is modeled using Menter-SST model with suppressed turbulent kinetic energy k within laminar regions. Overall more than 10100 operating points were investigated. The results are compared to literature when available and usually extend the information provided therein to a wider set of parameters. Explanations mostly focus on global eﬀects rather than detailed boundary layer investigations. Although validation with various sources shows good prediction quality of the method regarding integrated values and their tendencies there are diﬀerences in boundary layer properties compared to well resolved Large Eddy Simulations and Direct Numerical Simulations. Similar to classical airfoil design suction side control conﬁgurations show stronger eﬀects regarding airfoil performance. Blowing close to the leading edge is eﬀective regarding drag reduction but sensitive to changes in operating point. Blowing on the suction side has to be applied more thoughtfully in order to achieve beneﬁcial results than blowing on the pressure side which is usually favorable. In general blowing improves performance with Reynolds number while suction has the opposite correlation. However, depending on the conﬁguration suction might still be better even for high Reynolds numbers within the scope of parameters considered. Eﬀects of tripping or relaminarization through control mechanisms are not investigated but likely to have an inﬂuence. Yet, some conﬁgurations are chosen explicitly in a way to either utilize a speciﬁc transition position in order to have the desired boundary layer conditions in control region or stay well downstream of natural transition position.},
	language = {en},
	author = {Fahland, G.},
	year = {2019},
	pages = {110},
	file = {Fahland_2019_MA.pdf:/user/hi205/Dokumente/Literatur/Fahland_2019_MA.pdf:application/pdf}
}

